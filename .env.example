# Since .env is gitignored, you can use .env.example to build a new `.env` file when you clone the repo.
# Keep this file up-to-date when you add new variables to \`.env\`.

# This file will be committed to version control, so make sure not to have any secrets in it.
# If you are cloning this repo, create a copy of this file named `.env` and populate it with your secrets.

# We use dotenv to load Prisma from Next.js' .env file
# @see https://www.prisma.io/docs/reference/database-reference/connection-urls
DATABASE_URL=file:./db.sqlite

# CLERK is used for authentication and authorization in the app
# @see https://dashboard.clerk.dev for your Clerk API keys
# 
### NEW KEYS FOR APPS AFTER 1/18/2023
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_XXXXXXXXXXXXXXXXXXXX
CLERK_SECRET_KEY=sk_test_XXXXXXXXXXXXXXXXXXXXXXXX

TIMESERIES_DB_HOST=YOUR_HOST
TIMESERIES_DB_PORT=YOUR_PORT
TIMESERIES_DB_USERNAME=YOUR_USERNAME
TIMESERIES_DB_NAME=YOUR_DB_NAME
# Ensure that the password is wrapped in single quotes
TIMESERIES_DB_PASSWORD='YOUR_PASSWORD'
# Ensure that the connection string is wrapped in single quotes
TIMESERIES_DB_CONNECTION_STRING='YOUR_CONNECTION_STRING'

SENDGRID_API_KEY=YOUR_API_TOKEN

SENTRY_IGNORE_API_RESOLUTION_ERROR=0
SENTRY_DSN=YOUR_SENTRY_DSN

NEXT_PUBLIC_MAPBOX_PUBLIC_TOKEN=YOUR_PUBLIC_TOKEN
MAPBOX_API_TOKEN=YOUR_API_TOKEN

SPAN_API_TOKEN=YOUR_API_TOKEN